        const galleryContainer = document.getElementById("galleryContainer");

        const imagePaths = [
            "https://raw.githubusercontent.com/WiserJewelry/Designs/refs/heads/main/portfolio/ftp%20rendered.PNG",  // Add your own image names here
            "https://raw.githubusercontent.com/WiserJewelry/Designs/refs/heads/main/portfolio/jetsetter%203.PNG",
